# Application
NEXT_PUBLIC_APP_ENV=staging
NEXT_PUBLIC_APP_URL=https://staging.foodbook.psinfoodservice.com

# API Configuration
FOODBOOK_API_URL=https://staging-api.psinfoodservice.com
FOODBOOK_API_TIMEOUT=15000

# Authentication
JWT_SECRET=your-staging-jwt-secret-here-min-32-chars
COOKIE_DOMAIN=.psinfoodservice.com
SESSION_DURATION=86400

# Permalink Security
PERMALINK_SECRET=your-staging-permalink-secret-here
PERMALINK_MAX_AGE=600

# Caching (shorter for staging)
CACHE_REVALIDATE=60
CACHE_STALE_WHILE_REVALIDATE=120

# Feature Flags
FEATURE_IMPACT_SCORE=true
FEATURE_PDF_GENERATION=true

# Monitoring
NEXT_PUBLIC_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
SENTRY_ORG=your-org
SENTRY_PROJECT=ps-foodbook-staging

# Analytics
NEXT_PUBLIC_GTM_ID=GTM-STAGING
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-STAGING

# Logging
LOG_LEVEL=debug
